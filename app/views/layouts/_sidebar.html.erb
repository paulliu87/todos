<% if current_user %>
<h3>Recently completed</h3>
<% @recent_todos.each do |todo| %>
  <a href="/users/<%= params[:user_id] %>/todos/<%= todo.id %>" recent-list-id="<%= todo.id %>" class="list-group-item list-group-item-action clearfix list-group-item-success">
    <span class="glyphicon glyphicon-ok-sign"></span>
    <%= todo.title %>
    <span class="pull-right">
      <span class="glyphicon glyphicon-edit" style='margin-right:0.5em; display:inline-block'></span>
    </span>
  </a>
<% end %>
    <ul>
      <li>this is a test</li>
    </ul>
<% end %>
